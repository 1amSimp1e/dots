{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "markdown"
  ],
  // Disable COC NVIM diagnostic and replaced with lspconfig
  "diagnostic.enable": false,
  // Icons for diagnostic:
  "codeLens.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.enableMessage": "jump",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  // Icons for Coc-NVIM completions
  "suggest.completionItemKindLabels": {
    "around": "\ue796", //  Around
    "keyword": "\uf80a", //  key
    "variable": "\uf6a6", //  isometric box
    "value": "\uf89f", //  digits 123
    "operator": "\ufb26", // ﬦ lambda symbol
    "constructor": "\uf0ad", //  wrench
    "function": "\uf794", //  latin small letter F
    "reference": "\ufa46", // 渚 retweet symbol
    "constant": "\uf8fe", //  greek letter pi
    "method": "\u0192", // ƒ facebook "f"
    "struct": "\ufb44", // פּ file tree
    "class": "\uf0e8", //  hierarchy / site map
    "interface": "\uf417", //  git commit / line interrupted by circle
    "text": "\ue612", //  left-justified lines
    "enum": "\uf02c", //  pair of tags
    "enumMember": "\uf02b", //  tag
    "module": "\uf8d6", //  package icons
    "color": "\ue22b", //  palette
    "property": "\ue624", //  predator dots
    "field": "\uf9be", // 料left-justified sorted lines
    "unit": "\uf475", //  negative-space ellipsis
    "event": "\uf0e7", //  lightning bolt
    "file": "\uf15b", //  file / blank page
    "folder": "\uf07b", //  folder
    "snippet": "\ufb0c", // ﬌ arrow directory arrow
    "snippets-source": "\ue60b", //  pair of curly braces
    "default": "\uf29c" //  question mark in circle
  }
}
