{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "markdown"
  ],
  // Disable COC NVIM diagnostic and replaced with lspconfig
  "diagnostic.enable": false,
  // Icons for diagnostic:
  "codeLens.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.enableMessage": "jump",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  // Icons for Coc-NVIM completions
  "suggest.completionItemKindLabels": {
    "Namespace": "凜",
    "Around": "凌",
    "buffers": "﬘",
    "TypeParameter": "",
    "keyword": "",
    "variable": "",
    "value": "",
    "operator": "ﬦ",
    "constructor": "",
    "function": "",
    "reference": "",
    "constant": "",
    "method": "",
    "struct": "",
    "class": "",
    "interface": "ﰮ",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "color": "",
    "property": "",
    "field": "",
    "unit": "ﲽ",
    "event": "",
    "file": "",
    "folder": "",
    "snippet": "﬌",
    "default": ""
  },
  "Lua.telemetry.enable": true
}
