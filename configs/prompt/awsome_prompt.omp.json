{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#F1FA8C",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "<#42E66C>\uf135 </> <b>\ue5ff {{ .Path }}</b> ",
          "type": "path"
        },
        {
          "foreground": "#43CCEA",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": "<#ffffff>\u26A1</><b> {{ .HEAD }}</b>",
          "type": "git"
        },
        {
          "foreground": "",
          "style": "diamond",
          "template": "",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#e95678",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
